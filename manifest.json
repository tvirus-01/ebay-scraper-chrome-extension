{
	"manifest_version": 2,
	"name": "eBay Scraper Extension",
	"description": "Scrape product list from ebay website",
	"version": "1.0.0",
	"icons": {"128":"web-crawler.png"},
	"browser_action": {
		"default_icon": "web-crawler-19.png",
		"default_popup": "redirect.html"
	},
	"permissions": ["activeTab","storage", "http://best-price-web.herokuapp.com/*"]
}    